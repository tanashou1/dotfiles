  "rust-analyzer.cargo.buildScripts.enable": true,
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "state:open repo:${owner}/${repository} sort:created-desc assignee:@me"
    },
    {
      "label": "Recent Issues",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "githubIssues.issueBranchTitle": "issue${issueNumber}",
  "githubPullRequests.remotes": ["origin"],
  "workbench.activityBar.visible": true,
  "errorLens.enabledDiagnosticLevels": ["warning", "info", "error"],
  "errorLens.enabled": false,
  "extensions.autoUpdate": false,
  "markdown.marp.enableHtml": true,
  "markdown-image.base.fileNameFormat": "${timestamp}",
