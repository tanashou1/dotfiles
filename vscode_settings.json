{
  "debug.inlineValues": true,
  "files.eol": "\n",
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.bracketPairColorization.enabled": false,
  "vim.visualstar": true,
  // easymotion関連
  "vim.hlsearch": false,
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.easymotionKeys": "qwertyuiopasdfghjklzxcvbnm",
  "vim.highlightedyank.enable": true,
  "vim.visualModeKeyBindings": [
    {
      "before": ["ctrl+j"],
      "after": ["g", "b"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Esc>"],
      "commands": [":nohl"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["g", "j"],
      "after": ["j"]
    },
    {
      "before": ["g", "k"],
      "after": ["k"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["J"],
      "after": ["ctrl+d"]
    },
    // {
    //     "before": ["V","J"],
    //     "after": ["V","ctrl+d"]
    // },
    {
      "before": ["K"],
      "after": ["ctrl+u"]
    },
    // {
    //     "before": ["v","K"],
    //     "after": ["ctrl+u"]
    // },
    {
      "before": ["U"],
      "after": ["ctrl+r"]
    },
    {
      "before": ["ctrl+j"],
      "after": ["g", "b"]
    }
  ],
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Pylance",
  "python.analysis.typeCheckingMode": "basic",
